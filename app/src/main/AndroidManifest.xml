<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />
    <uses-permission android:name="android.permission.SCHEDULE_EXACT_ALARM" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ChAD"
        tools:targetApi="31">
        <receiver android:name=".Notification"
            android:enabled="true"
            android:exported="true" />
        <activity
            android:name="MainActivity.add_act_popup"
            android:exported="false" />
        <activity
            android:name="MainActivity.add_sleep_popup"
            android:exported="false" />
        <activity
            android:name="MainActivity.login"
            android:exported="false" />
        <activity
            android:name="MainActivity.addMain"
            android:exported="false" />
        <activity
            android:name="MainActivity.calendarMain"
            android:exported="false" />
        <activity
            android:name="MainActivity.addmed"
            android:exported="false" />
        <activity
            android:name="MainActivity.medsMain"
            android:exported="false" />
        <activity
            android:name="MainActivity.scroll"
            android:exported="false" />
        <activity
            android:name="MainActivity.add"
            android:exported="false" />
        <activity
            android:name="MainActivity.contactsMain"
            android:exported="false" />
        <activity
            android:name="MainActivity.MainActivity"
            android:exported="false" />
        <activity
            android:name="MainActivity.register"
            android:exported="true"
            android:theme="@style/Theme.ChAD"
            tools:ignore="DuplicateActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>